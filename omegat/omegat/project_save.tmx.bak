    <tu>
      <tuv lang="en-US">
        <seg>Guybrush?!</seg>
      </tuv>
      <tuv lang="pl">
        <seg>Guybrushu?!</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en-US">
