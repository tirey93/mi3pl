msgctxt "sggt475"
msgid "It's the hole that I dug Elaine up from."
msgstr "To dziura, z której wykopałem Elaine."


msgctxt "sggt476"
msgid "Elaine must be buried underneath the stage!"
msgstr "Elaine musi być zakopana pod sceną!"


msgctxt "sggt477"
msgid "I already got Elaine."
msgstr "Już wykopałem Elaine."


msgctxt "sggt478"
msgid "I can't dig it up with my bare hands!"
msgstr "Nie mogę kopać gołymi rękoma!"


msgctxt "sggt479"
