msgstr "Zabierz to ode mnie. To odrażające."


msgctxt "bisq485"
msgid "I'd like a crewcut, please."
msgstr "Na jeżyka proszę."


msgctxt "bisq486"
msgid "Make my head shine like a cue ball."
msgstr "Chcę być łysy jak kolano."


msgctxt "bisq487"
msgid "Shave me like a yak."
msgstr "Ostrzyż mnie jak barana."


msgctxt "bisq488"
msgid "Give me that Mexican Hairless look, please."
msgstr "Chcę wyglądać jak wygolony wyżeł."


msgctxt "bisq489"
msgid "Make me balder than Largo LaGrande."
msgstr "Chcę być bardziej łysy niż Largo LaGrande."


msgctxt "bihm490"
msgid "Aye Aye, Captain Rottingham!"
msgstr "Tak jest, kapitanie Rottingham."


msgctxt "bihm491"
msgid "Whatever you say, sir!"
msgstr "Wedle życzenia, psze pana."


msgctxt "bihm492"
msgid "Your wish is my command!"
msgstr "Pańskie życzenie jest dla mnie rozkazem."


msgctxt "birh493"
