msgctxt "c3bl312"
msgid "Hey!"
msgstr ""


msgctxt "c3bl313"
msgid "Look guys!"
msgstr "Patrzcie!"


msgctxt "c3bl314"
msgid "A whale!"
msgstr "Wieloryb!"


msgctxt "c3hm315"
msgid "Where?"
msgstr "Gdzie?"


msgctxt "c3vh316"
msgid "Where?"
msgstr "Gdzie?"


msgctxt "c3gt317"
msgid "That ship is gaining on us."
msgstr "Ten statek nas dogania."


msgctxt "c3gt318"
msgid "Cutthroat Bill!"
msgstr "Bill!"


msgctxt "c3gt319"
msgid "Rig the topsail!"
msgstr "Takluj marsel!"


msgctxt "c3vh320"
msgid "Is that a Right whale?"
msgstr "Czy to waleń?"


msgctxt "c3hm321"
msgid "No no, lad."
msgstr "Nie, chłopcze."


