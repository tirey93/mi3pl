msgctxt "binz547"
msgid "portrait"
msgstr "portret"


msgctxt "bigt548"
msgid "Why would I want something that ugly?"
msgstr "Po co mi taki bohomaz?"


msgctxt "binz549"
msgid "parrot painting"
msgstr "obraz papugi"


msgctxt "bigt550"
